<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form action="#" method="post">
        Firstname: <input type="text" id="fname">
        <br><br>
        lastname: <input type="text" id="lname">
        <br><br>
        <button onclick="compare()">Done</button>
    </form>

    <script type="text/javascript">
        // window.alert("welcome");

        function compare(){
            let inputFirstName = document.getElementById("fname");

            let inputLastName = document.getElementById("lname");

            if(inputFirstName.value == inputLastName.value){
                window.alert("Password Matched");
            }
            else{
                window.alert("Password do not match");
                return false;
            }
        };
    </script>
</body>
</html>